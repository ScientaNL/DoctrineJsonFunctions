{
	"name": "scienta/doctrine-json-functions",
	"abandoned": "scienta/doctrine-json-functions",
	"type": "library",
	"description": "A set of extensions to Doctrine 2 that add support for json query functions.",
	"keywords": ["doctrine", "orm", "json", "dql", "database", "mysql", "postgres", "postgresql", "mariadb"],
	"license": "MIT",
	"authors": [
		{
			"name": "Doctrine Json Functions Contributors",
			"homepage": "https://github.com/ScientaNL/DoctrineJsonFunctions/contributors"
		}
	],
	"require": {
		"php": "^7.1",
		"ext-pdo": "*"
	},
	"require-dev": {
		"doctrine/orm": "~2.6",
		"doctrine/coding-standard": "^5.0",
		"phpunit/phpunit": "^6.5"
	},
	"autoload": {
		"psr-4": {
			"Syslogic\\DoctrineJsonFunctions\\" : "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Doctrine\\Tests\\": "tests/Doctrine/Tests",
			"Syslogic\\DoctrineJsonFunctions\\Tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0-dev"
		}
	}
}
